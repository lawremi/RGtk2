      <parameters>
        <parameter name="scaled_font" transfer-ownership="none">
          <doc xml:space="preserve"
               filename="cairo.h"
               line="1703">the scaled-font being created</doc>
          <type name="_scaled_font_t" c:type="cairo_scaled_font_t*"/>
        </parameter>
        <parameter name="cr" transfer-ownership="none">
          <doc xml:space="preserve"
               filename="cairo.h"
               line="1704">a cairo context, in font space</doc>
          <type name="_t" c:type="cairo_t*"/>
        </parameter>
        <parameter name="extents" transfer-ownership="none">
          <doc xml:space="preserve"
               filename="cairo.h"
               line="1705">font extents to fill in, in font space</doc>
